{
  "name": "django-angular-admin",
  "version": "1.0.0",
  "description": "Django Angular Admin",
  "scripts": {
    "install": "cd ./bash/ && ./install.sh",
    "python-virtualenv": "cd ./back/ && virtualenv --python=python2 .env",
    "createsuperuser": "cd ./bash/ && ./createsuperuser.sh",
    "migrations": "cd ./bash/ && ./migrations.sh",
    "start": "npm run start-backend & npm run start-frontend",
    "start-backend":  "cd ./back/ && source .env/bin/activate && python manage.py runserver",
    "start-frontend": "cd ./front/ && npm start"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RodrigoRiquelme/django-angular-admin.git"
